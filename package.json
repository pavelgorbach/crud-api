{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start:dev": "nodemon --watch 'src/**' --ext 'ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node src/index.ts'",
    "start:prod": "npm run build && node dist/main.mjs",
    "start:multi": "multi",
    "build": "webpack --mode=production --node-env=production --config webpack.config.cjs",
    "lint": "TIMING=1 eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavelgorbach/crud-api.git"
  },
  "keywords": [
    "crud",
    "nodejs",
    "rss"
  ],
  "author": "polgorbach@gmail.com",
  "bugs": {
    "url": "https://github.com/pavelgorbach/crud-api/issues"
  },
  "homepage": "https://github.com/pavelgorbach/crud-api#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@webpack-cli/generators": "^3.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
